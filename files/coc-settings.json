{
  "[css][markdown][typescript][typescriptreact][json][astro][rust]": {
    "coc.preferences.formatOnSave": true
  },
  "eslint.enable": true,
  "prettier.enable": true,
  "signature.enable": false,
  "signature.hideOnTextChange": true,
  "suggest.autoTrigger": "trigger",
  "suggest.noselect": true,
  "suggest.insertMode": "insert",
  "codeLens.enable": false,
  "smartf.timeout": 0,
  "workspace.ignoredFolders": ["$HOME", "$HOME/.cargo/**", "$HOME/.rustup/**"],
  "rust-analyzer.disableProgressNotifications": true,
  "tsserver.log": "verbose"
}
